const readlineSync = require("readline-sync");

const geomtry = Number(readlineSync.question("Input Geometry grade: "));
const algebra = Number(readlineSync.question("Input Algebra grade: "));
const physics = Number(readlineSync.question("Input Physics grade: "));

const sum = geomtry + algebra + physics;
const avg = sum / 3;
console.log(`The average of these grades is: ${avg}`);

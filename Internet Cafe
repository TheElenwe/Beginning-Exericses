const readlineSync = require("readline-sync");

 let hoursSpend = readlineSync.question("Input how many hours the user spend: ");
 const userStatus = readlineSync.question("You are member? Press yes,if u are not member press no: ");

if (userStatus === "yes") {
  console.log(hoursSpend * 2 * 1.1);
} else if (userStatus === "no") {
  console.log(hoursSpend * 5 * 1.2);
}
